{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://wordpress.org/schema/post.json",

	"title": "A post object.",
	"description": "",

	"type": "object",

	"properties": {
		"version": {
			"description": "The schema version that this post object adheres to.",
			"type": "integer",
			"const": 1
		},
		"id": {
			"description": "The post ID. It must be a positive integer.",
			"type": "integer",
			"minimum": 1
		},
		"title": {
			"description": "",
			"type": "string"
		},
		"slug": {
			"description": "",
			"type": "string"
		},
		"author": {
			"description": "",
			"type": "integer",
			"minimum": 1
		},
		"type": {
			"description": "",
			"type": "string"
		},
		"postStatus": {
			"description": "",
			"type": "string"
		},
		"published": {
			"description": "",
			"type": "string",
			"format": "date-time"
		},
		"modified": {
			"description": "",
			"type": "string",
			"format": "date-time"
		},
		"terms": {
			"type": "array",
			"items": {
				"type": "number",
				"minimum": 1
			}
		},
		"parent": {
			"description": "",
			"type": "integer",
			"minimum": 1
		},
		"password": {
			"description": "",
			"type": "string"
		},
		"menuOrder": {
			"description": "",
			"type": "integer"
		},
		"sticky": {
			"description": "",
			"type": "boolean"
		},
		"commentsOpen": {
			"description": "",
			"type": "boolean"
		},
		"pingsOpen": {
			"description": "",
			"type": "boolean"
		},
		"content": {
			"description": "",
			"$ref": "https://wordpress.org/schema/content.json"
		},
		"excerpt": {
			"description": "",
			"$ref": "https://wordpress.org/schema/content.json"
		},
		"attachmentUrl": {
			"description": "",
			"type": "string"
		},


		"meta": {
			"description": "An array of post metadata.",
			"$ref": "https://wordpress.org/schema/meta.json"
		}
	},

	"additionalProperties": false,
	"required": [ "version" ]
}
